accountService.isAuthenticated();\n  }\n\n  login() {\n    this.modalRef = this.loginModalService.open();\n  }\n\n  ngOnDestroy() {\n    if (this.authSubscription) {\n      this.eventManager.destroy(this.authSubscription);\n    }\n  }\n}\n"]}]}